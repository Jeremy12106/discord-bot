bot:
  # 機器人前墜
  prefix: "!"
  # 機器人線上狀態 (online, idle, dnd, invisible)
  status: "online"  
  # 機器人活動狀態
  activity: null

llm:
  # 機器人聊天API類型 (gemini, github)
  gpt_api: "gemini"
  # 機器人聊天模型 (gemini-1.5-flash, gemini-2.0-flash, ...)
  model: "gemini-2.0-flash"
  # 機器人系統提示語
  system_prompt: "[使用繁體中文回答]"
  # 機器人預設個性
  personality: "你的名字是豆白"
  # 機器人是否啟用聊天記憶
  chat_memory: true
  # 機器人是否啟用搜尋引擎
  use_search_engine: true

music:
  # 是否顯示播放進度條
  display_progress_bar: true
  # YT 搜尋顯示數量
  search_count: 10
  # 音樂播放長度限制 (秒)
  time_limit: 1800
  # 音樂品質設定
  before_options: null
  options: "-ar 48000 -ac 2"

radio_stations:
  - name: "讀書電台"                            # 電台名稱
    url: "https://example.com/radio/reading"   # 電台網址
    description: "By Lofi Girl"                # 電台描述，選填
    emoji: "📚"                                # 表情符號，選填
  
  - name: 
    url: 
    description: 
    emoji: 